<%= form_for(:answers, url: check_with_answer_question_path(rand_q)) do |f| %>
    <%= f.label rand_q.answers.keys.first.to_sym, rand_q.answers.keys.first %>
    <%= f.text_field rand_q.answers.keys.first.to_sym %>
    <%= f.submit "Submit Answers" %>
<% end %>


<% rand_q.answers.keys.each do |key| %>
  <%= f.label key.to_sym, key %>
  <%= f.text_field key.to_sym %>
  <span></span>
<% end %>
